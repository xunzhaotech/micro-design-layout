{
  "name": "micro-design-layout",
  "description": "Micro Design Layout of Vue, easy to use Micro scaffolding.",
  "version": "1.0.2",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "style": "./dist/style.css",
  "main": "./dist/micro-layout.umd.js",
  "module": "./dist/micro-layout.es.js",
  "exports": {
    ".": {
      "import": "./dist/micro-layout.es.js",
      "require": "./dist/micro-layout.umd.js"
    },
    "./dist/style.css": "./dist/style.css",
    "./dist/style.less": "./dist/style.less"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xunzhaotech/micro-design-layout.git"
  },
  "contributors": [
    "dasheng<1525572900@qq.com>"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "build:types": "tsc -b --force tsconfig.esm.json",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs"
  },
  "types": "./dist",
  "peerDependencies": {
    "ant-design-vue": ">=2.2.0",
    "vue": ">=3.2.0",
    "vue-router": "^4.0.3"
  },
  "dependencies": {
    "ant-design-vue": "^2.2.8",
    "lodash-es": "^4.17.21",
    "omit.js": "^2.0.2",
    "vue": "^3.2.0",
    "vue-router": "^4.0.10"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.4",
    "@types/node": "^16.0.1",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "@vitejs/plugin-vue": "^1.6.0",
    "@vitejs/plugin-vue-jsx": "^1.1.7",
    "@vue/compiler-sfc": "^3.2.0",
    "eslint": "^7.30.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-vue": "^7.13.0",
    "fs-extra": "^10.0.0",
    "less": "^4.1.1",
    "prettier": "^2.3.2",
    "tsc": "^2.0.3",
    "tsup": "^4.12.5",
    "typescript": "^4.3.2",
    "vite": "^2.5.6",
    "vite-dts": "^1.0.3",
    "vite-plugin-dts": "^0.5.2",
    "vue-tsc": "^0.3.0",
    "vuepress": "^2.0.0-beta.27"
  }
